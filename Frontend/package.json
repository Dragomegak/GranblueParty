{
  "name": "gbf-frontend",
  "version": "2.1.0",
  "description": "Granblue Party Frontend",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=production node server.js",
    "build:server": "cross-env NODE_ENV=production webpack --config webpack.server.config.js",
    "build:client": "cross-env NODE_ENV=production webpack --config webpack.client.config.js",
    "build:static": "cross-env NODE_ENV=production webpack --config webpack.static.config.js",
    "build": "npm run build:server && npm run build:client && npm run build:static",
    "dev:server": "webpack -w --config webpack.server.config.js",
    "dev:client": "webpack-dev-server --config webpack.client.config.js",
    "dev:static": "webpack --config webpack.static.config.js",
    "nodemon": "nodemon --trace-warnings server.js"
  },
  "author": {
    "name": "Minimalist",
    "email": "contact@granblue.party",
    "url": "https://www.granblue.party"
  },
  "license": "GPL-3.0",
  "devDependencies": {
    "autoprefixer": "^9.7.5",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.2",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "extract-css-chunks-webpack-plugin": "^4.7.4",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^3.2.0",
    "ifdef-loader": "^2.1.5",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "nodemon": "^2.0.2",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "purgecss-webpack-plugin": "^1.6.0",
    "sitemap-webpack-plugin": "^0.7.1",
    "vue-loader": "^15.9.1",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.42.1",
    "webpack-cdn-plugin": "^3.2.2",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-regular-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/vue-fontawesome": "^0.1.8",
    "axios": "^0.19.2",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "remove-files-webpack-plugin": "^1.4.0",
    "tailwindcss": "^1.2.0",
    "vue": "^2.6.11",
    "vue-router": "^3.1.6",
    "vue-server-renderer": "^2.6.11",
    "vuex": "^3.1.3"
  }
}
