{
  "name": "gbf-frontend",
  "version": "2.1.0",
  "description": "Granblue Party Frontend",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=production node server.js",
    "build:server": "cross-env NODE_ENV=production webpack --config webpack.server.config.js",
    "build:client": "cross-env NODE_ENV=production webpack --config webpack.client.config.js",
    "build:static": "cross-env NODE_ENV=production webpack --config webpack.static.config.js",
    "build": "npm run build:server && npm run build:client && npm run build:static",
    "dev:server": "webpack -w --config webpack.server.config.js",
    "dev:client": "webpack-dev-server --config webpack.client.config.js",
    "dev:static": "webpack --config webpack.static.config.js",
    "nodemon": "nodemon --trace-warnings server.js"
  },
  "author": {
    "name": "Minimalist",
    "email": "contact@granblue.party",
    "url": "https://www.granblue.party"
  },
  "license": "GPL-3.0",
  "devDependencies": {
    "autoprefixer": "^10.0.1",
    "copy-webpack-plugin": "^6.2.1",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.0",
    "cssnano": "^4.1.10",
    "extract-css-chunks-webpack-plugin": "^4.7.5",
    "file-loader": "^6.1.1",
    "html-webpack-plugin": "^4.5.0",
    "ifdef-loader": "^2.1.5",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "nodemon": "^2.0.6",
    "postcss": "^8.1.2",
    "postcss-import": "^13.0.0",
    "postcss-loader": "^4.0.4",
    "sitemap-webpack-plugin": "^0.8.1",
    "vue-loader": "^15.9.3",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.44.2",
    "webpack-cdn-plugin": "^3.3.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.2.0",
    "webpack-node-externals": "^2.5.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-regular-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/vue-fontawesome": "^2.0.0",
    "axios": "^0.20.0",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "remove-files-webpack-plugin": "^1.4.4",
    "tailwindcss": "^1.9.5",
    "vue": "^2.6.12",
    "vue-router": "^3.4.7",
    "vue-server-renderer": "^2.6.12",
    "vuex": "^3.5.1"
  }
}
